{
    "version": 2,
    "builds": [
        {
            "src": "src/api/index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/api/index.js"
        },
        {
            "src": "/",
            "dest": "/dist/$1"
        }
    ]
}
