{
    "version": 2,
    "builds": [
        {
            "src": "src/api/index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/api/(.*)",
            "dest": "src/api/index.js"
        },
        {
            "src": "/",
            "dest": "index.html"
        }
    ]
}
